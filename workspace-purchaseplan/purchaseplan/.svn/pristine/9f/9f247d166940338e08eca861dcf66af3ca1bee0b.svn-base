package com.youzhicai.purchaseplan.vo;

import java.util.List;

import com.youzhicai.purchaseplan.entity.PairModel;
import com.youzhicai.purchaseplan.entity.PurchasePlanList;

public class PurchasePlanListVO extends PurchasePlanList {

    private List<PairModel> pairList;

    private List<PurchaseGoodsSupplyVO> goodsSupplyList;

    /** 本地物资信息状态 1:本地;2:物资库 **/
    private Integer isLocal;

    public List<PairModel> getPairList() {
        return pairList;
    }

    public void setPairList(List<PairModel> pairList) {
        this.pairList = pairList;
    }

    public Integer getIsLocal() {
        return isLocal;
    }

    public void setIsLocal(Integer isLocal) {
        this.isLocal = isLocal;
    }

    public List<PurchaseGoodsSupplyVO> getGoodsSupplyList() {
        return goodsSupplyList;
    }

    public void setGoodsSupplyList(List<PurchaseGoodsSupplyVO> goodsSupplyList) {
        this.goodsSupplyList = goodsSupplyList;
    }

}