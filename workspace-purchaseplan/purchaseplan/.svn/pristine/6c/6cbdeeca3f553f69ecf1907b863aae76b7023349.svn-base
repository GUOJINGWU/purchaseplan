package com.youzhicai.purchaseplan.enums;

public enum BasicEnums {
    DEFAULT(0, "默认"), SUCCESS(1, "成功"), FAILURE(2, "失败"), REMOVE(-2, "删除"), MATERIALSTORESUCCESS(3, "物资库存在"), MATERIALSTOREFAILURE(4, "物资库不存在");
    private int status;
    private String value;

    private BasicEnums(int status, String value) {
        this.status = status;
        this.value = value;
    }

    public int getStatus() {
        return status;
    }

    public String getValue() {
        return value;
    }
}
