.autocomplete-suggestions { color: #333333; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; border: 1px solid #c5c5c5; background: #FFF; cursor: default;overflow: hidden; -webkit-box-shadow: 1px 4px 3px rgba(50, 50, 50, 0.64); -moz-box-shadow: 1px 4px 3px rgba(50, 50, 50, 0.64); box-shadow: 1px 4px 3px rgba(50, 50, 50, 0.64); }
.autocomplete-suggestion { padding: 2px 5px; white-space: nowrap; overflow: hidden; }
.autocomplete-no-suggestion { padding: 2px 5px;}
body[yzc-skin-buyer] .autocomplete-selected { background: #2d8ff7; color:#fff;}
 .autocomplete-selected { background: #2d8ff7; color:#fff;}
.autocomplete-suggestions strong { font-weight: bold; color: #000; }
.autocomplete-selected strong { font-weight: bold; color: #fff; }
.autocomplete-group { padding: 2px 5px; }
.autocomplete-group strong { font-weight: bold; font-size: 16px; color: #000; display: block; border-bottom: 1px solid #000; }