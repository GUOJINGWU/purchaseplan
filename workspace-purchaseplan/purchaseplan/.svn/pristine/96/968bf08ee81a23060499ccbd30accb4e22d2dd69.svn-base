<html xmlns:th="http://www.thymeleaf.org">
<head>
<meta charset="utf-8" />
<title>addMaterial</title>
<link href="//cdn.ukzhicai.com/layui/css/layui.css" rel="stylesheet" type="text/css" />
<link rel="stylesheet" th:href="@{/res/css/dialog-content.css}" />
<script src="https://apps.bdimg.com/libs/jquery/1.9.1/jquery.min.js"></script>
<script type="text/javascript" src="//cdn.youzhicai.com/layui/layui.js"></script>
</head>
<body>
	<div class="inner-content yzc-clearfix">
	
        <h3 class="add-member-title">添加物资</h3>
        	<form class="layui-form plan-form" action=""  style="padding-top: 20px">
                       
                        	<div class="layui-form-item plan-item" style="position:relative">


                           		 <div class="layui-inline" style="width: 280px">
                                	<label class="plan-label">物资名称/编码：
                               		 </label>

                                		<div class="layui-input-block layui-input2"
                                     		style="display: inherit;margin-left: 0;position: relative;">
                                    		<input lay-verify="title" autocomplete="off"
                                           class="layui-input  input-clear"
                                           placeholder="请输入物资名称/编码"
                                           type="text" style="width: 172px;display: inline-block;" />
                                  
                                		</div>
                            	</div>
                            	
                            	 <div class="layui-inline" >
		                                <label class="plan-label">所属分类：</label>
		                                <div class="layui-input-block  showform limit-select"
		                                     style="margin-left: 0px !important;display: inline-block;">
		                                    <select name="" class="plan-type">
		                                        <option value="11">11</option>
		                                        <option value="22">22</option>
		                                        <option value="33">33</option>
		                          
		                                    </select>
		
		                                </div>
		                                <div class="layui-input-block  showform limit-select"
		                                     style="margin-left: 0px !important;display: inline-block;">
		                                    <select name="" class="plan-type">
		                                        <option value="11">11</option>
		                                        <option value="22">22</option>
		                                        <option value="33">33</option>
		                          
		                                    </select>
		
		                                </div>
		                                <div class="layui-input-block  showform limit-select"
		                                     style="margin-left: 0px !important;display: inline-block;">
		                                    <select name="" class="plan-type">
		                                        <option value="11">11</option>
		                                        <option value="22">22</option>
		                                        <option value="33">33</option>
		                          
		                                    </select>
		
		                                </div>
		                         </div>
                            	

                            
		                        
		                            
		                            <a class="plan-white-btn plan-search-btn">搜索</a>
		                         	
		
		                         
		                       </div>
		                        
            
                    </form>
                    	
       
       
       					
									<table class="layui-table">
										<colgroup>
											<col width="6%" />
											<col width="15%" />
											<col width="10%" />
											<col width="9%" />
											<col width="9%" />
											<col width="6%" />
											<col width="8%" />
											<col width="18%" />
											<col width="10%" />
										</colgroup>
										<thead>
											<tr>
												<th class="yzc-aCenter">序号</th>
												<th class="yzc-aCenter">物资编号/名称</th>
												<th class="yzc-aCenter">材质/品牌</th>
												<th class="yzc-aCenter">规格/型号</th>
												<th class="yzc-aCenter">其他属性</th>
												<th class="yzc-aCenter">单位</th>
												<th class="yzc-aCenter">补充说明</th>
												<th class="yzc-aCenter">需求数量</th>
												<th class="yzc-aCenter">操作</th>
											</tr>
										</thead>
										<tbody>
											<tr>
												<td class="yzc-aCenter">1</td>
												<td class="yzc-aCenter">001</td>
												<td class="yzc-aCenter">001</td>
												<td class="yzc-aCenter">001</td>

												<td class="yzc-aCenter">001</td>
												<td class="yzc-aCenter">个</td>

												<td class="yzc-aCenter"></td>
												<td class="yzc-aCenter">


													<ul class="btn-numbox">

														<li>
															<ul class="count">
																<li>
																	<span id="num-jian" class="num-jian">-</span>
																</li>
																<li>
																	<input type="text" class="input-num" id="input-num" value="0" />
																</li>
																<li>
																	<span id="num-jia" class="num-jia">+</span>
																</li>
															</ul>
														</li>


													</ul>
												</td>
												<td class="yzc-aCenter">

													<a href="" class="plan-a">取消</a>

												</td>
											</tr>
											<tr>
												<td class="yzc-aCenter">2</td>
												<td class="yzc-aCenter">001</td>
												<td class="yzc-aCenter">001</td>
												<td class="yzc-aCenter">001</td>

												<td class="yzc-aCenter">001</td>
												<td class="yzc-aCenter">个</td>

												<td class="yzc-aCenter"></td>
												<td class="yzc-aCenter">

													<ul class="btn-numbox">
                              
														<li>
															<ul class="count">
																<li>
																	<span id="num-jian1" class="num-jian">-</span>
																</li>
																<li>
																	<input type="text" class="input-num" id="input-num1" value="0" />
																</li>
																<li>
																	<span id="num-jia1" class="num-jia">+</span>
																</li>
															</ul>
														</li>
													</ul>
												</td>
												<td class="yzc-aCenter">

													<a href="" class="plan-a">取消</a>

												</td>
											</tr>

										</tbody>
									</table>
					<div class="layui-box layui-laypage layui-laypage-default yzc-fr" id="" style="">
                        <span class="layui-laypage-curr">
                            <em class="layui-laypage-em"></em>
                            <em>1</em>
                 	    </span>
                        <a href="javascript:;" data-page="2">2</a>
                        <a href="javascript:;" data-page="3">3</a>
                        <a href="javascript:;" data-page="4">4</a>
                        <a href="javascript:;" data-page="5">5</a>
                        <span>…</span>
                        <a href="javascript:;" class="layui-laypage-last" title="尾页" data-page="100">末页</a>


                        <a href="javascript:;" class="layui-laypage-next" data-page="2">下一页</a>
                        <span class="layui-laypage-total">到第
                     	    <input min="1"  onkeyup="this.value=this.value.replace(/\D/, '');" value="1" class="layui-laypage-skip" type="number" /> 页
                                                                       
                            <button  type="button" class="layui-laypage-btn">确定</button>
                        
                        </span>
                    </div>
      					
						<a class="plan-white-btn close-btn">关闭</a>
									
						
                    </div>
                    
  
                    
    
     <script th:src="@{/res/js/plan.js}" type="text/javascript"></script>
     
      <script>
      
      $(function(){
    	  
    	  $(".close-btn").click(function(){
    	       	top.CloseDialog("add-material");
    	       })
    	    
      });
      
     
    
           
           
    
    </script>
</body>
</html>